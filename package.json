{
  "name": "td-react-snippets",
  "displayName": "td-react-snippets",
  "description": "基于dva的某些快捷命令补充",
  "version": "2.3.6",
  "publisher": "mizy",
  "icon": "images/logo.png",
  "keywords": [
    "snippets",
    "react",
    "redux",
    "react-native",
    "javascript",
    "typescript"
  ],
  "engines": {
    "vscode": "^1.11.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mizy/td-react-snipppets.git"
  },
  "categories": [
    "Snippets"
  ],
  "extensionKind": "ui",
  "main": "./out/index.js",
  "activationEvents": [
    "onCommand:extension.snippetSearch"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.snippetSearch",
        "title": "ES7 snippet search"
      }
    ],
    "keybindings": [
      {
        "command": "extension.snippetSearch",
        "key": "ctrl+alt+r",
        "mac": "shift+cmd+r",
        "when": "editorTextFocus"
      }
    ],
    "snippets": [
		{
			"language": "javascript",
			"path": "./out/addon.js"
		  },
      {
        "language": "javascript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/dva.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/dva.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "3.4.2",
    "vscode": "1.1.33",
    "tslint": "5.15.0",
    "@types/node": "11.13.0"
  }
}
