	"tntModel": {
		"prefix": "tntModel",
		"body": [
			"import { message } from \"antd\";",
			"import service from \"./service\";",
			"",
			"export default {",
			"\tnamespace: \"demo\",",
			"\tstate: {",
			"",
			"\t},",
			"\teffects: {",
			"\t\t*getSolutionList({ payload }, { call, put, select }) {",
			"\t\t\tconst { id } = yield select(state => state.demo);",
			"\t\t\tconst res = yield call(service.getSolutionList, payload);",
			"",
			"\t\t\tyield put({",
			"\t\t\t\ttype: \"updateState\",",
			"\t\t\t\tpayload: {",
			"\t\t\t\t\tlist: res",
			"\t\t\t\t}",
			"\t\t\t});",
			"\t\t}",
			"\t},",
			"\treducers: {",
			"\t\tupdateState(state, { payload }) {",
			"\t\t\treturn {",
			"\t\t\t\t...state,",
			"\t\t\t\t...payload",
			"\t\t\t};",
			"\t\t}",
			"\t}",
			"};",
			""
		],
		"description": "tntModel"
	},
